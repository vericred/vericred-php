{
  "name": "vericred/vericred-php",
  "version": "0.0.3",
  "description": "Autogenerated PHP client for the Vericred API",
  "keywords": [
    "swagger",
    "php",
    "sdk",
    "api"
  ],
  "homepage": "http://swagger.io",
  "license": "Apache v2",
  "authors": [
    {
      "name": "Vericred Inc.",
      "homepage": "http://vericred.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "satooshi/php-coveralls": "~1.0",
    "squizlabs/php_codesniffer": "~2.6"
  },
  "autoload": {
    "psr-4": {
      "Vericred\\Client\\": "lib/"
    }
  }
}